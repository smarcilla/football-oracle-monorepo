{
  "name": "@football-oracle/data-registry",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "test": "vitest run --exclude **/*.integration.test.ts",
    "test:integration": "vitest run **/*.integration.test.ts",
    "test:coverage": "vitest run --coverage",
