  "dependencies": {
    "@football-oracle/kafka": "workspace:*",
    "@football-oracle/types": "workspace:*",
    "@prisma/client": "^5.22.0",
    "express": "^5.0.0",
    "ioredis": "^5.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20.17.0",
    "@types/supertest": "^6.0.2",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.18"
  }
}
